{
    "name": "consolelogsuper",
    "displayName": "ConsoleLogSuper",
    "description": "A little extension that add a keybind for console log with extra identifiers",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.47.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:consolelogsuper.log"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "consolelogsuper.log",
                "title": "Insert console log"
            }
        ],
        "keybindings": [
            {
                "command": "consolelogsuper.log",
                "key": "ctrl+k j",
                "when": "editorTextFocus"
            }
        ],
        "configuration": {
            "title": "SuperConsoleLog",
            "properties": {
                "superConsoleLog.singleQuotes": {
                    "type": "boolean",
                    "default": false,
                    "description": "Use single quote or use double quote"
                },
                "superConsoleLog.endSemiColon": {
                    "type": "boolean",
                    "default": true,
                    "description": "Put a semi colon at the end of the console.log"
                },
                "superConsoleLog.includeFileName": {
                    "type": "boolean",
                    "default": true,
                    "description": "Put the file name in the console.log"
                },
                "superConsoleLog.includeEnclosureFuncName": {
                    "type": "number",
                    "default": 2,
                    "description": "Put the enclosing function name in the console.log"
                },
                "superConsoleLog.includeEnclosureClassName": {
                    "type": "number",
                    "default": 1,
                    "description": "Put the enclosing class  name in the console.log"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint src --ext ts",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.47.0",
        "@types/glob": "^7.1.1",
        "@types/mocha": "^7.0.2",
        "@types/node": "^13.11.0",
        "eslint": "^6.8.0",
        "@typescript-eslint/parser": "^2.30.0",
        "@typescript-eslint/eslint-plugin": "^2.30.0",
        "glob": "^7.1.6",
        "mocha": "^7.1.2",
        "typescript": "^3.8.3",
        "vscode-test": "^1.3.0"
    }
}